  "scripts": {
    "start": "react-scripts start",
    "netlify": "echo '/*    /index.html   200' > build/_redirects",
    "build": "react-scripts build && yarn netlify",
    "eject": "react-scripts eject",
    "coverage": "nyc npm test",
